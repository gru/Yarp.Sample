{
  "Logging": {
    "LogLevel": {
      "Default": "Trace"
    }
  },
  "ReverseProxy": {
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "localhost": {
            "Address": "http://localhost:5001/"
          }
        }
      }
    },
    "Routes": {
      "route1" : {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    }
  },
  "AllowedHosts": "*"
}
